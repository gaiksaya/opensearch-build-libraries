lib = library(identifier: 'jenkins@modify-s3-download', retriever: modernSCM([
    $class: 'GitSCMSource',
    remote: 'https://github.com/gaiksaya/opensearch-build-libraries-1.git',
]))

standardReleasePipeline()
{
    downloadFromS3(
        assumedRoleName: 'opensearch-bundle',
        roleAccountNumberCred: 'jenkins-aws-account-public',
        downloadPath: 'test/test1/peru.jpg',
        bucketName: 'opensearch-ci-test-buildbucket51d063ef-15e184swqkj7r',
        localPath: "test2/",
        force: true
    )
    sh "ls -l && ls -l test2"
}